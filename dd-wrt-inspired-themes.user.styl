        body > div:first-child {
            &::before {
                P: -2rem 0 calc(100vh - 3rem) 0, 0
                content: \
                    '  ⭐ BG: ' + luminosity(BG) + \
                    '  ⭐ FG: ' + luminosity(FG) + \
                    '  🌟 v00: ' + v00 + \
                    '  🌟 v50: ' + v50 + \
                    '  🌟 v90: ' + v90
                c: fg1 0 bg4
                b: 0 1px bg5

                text-align: center
                line-height: 2rem
            }
            #content {
                margin-top: 2rem
            }
            .logo {
                margin-top: 2.5rem
            }
        }
    }

    // Userstyle variables.
    :root {
        // Userstyle info.
        --note = 'DD-WRT Inspired Themes - 1.0.0'

        // Feat -> Custom monospace font.
        --f-mono: f_mono, Cascadia Code, Fira Code, SFMono-Regular, Consolas, \
                Liberation Mono, Menlo, monospace

        // Feat -> Custom user interface font.
        --f-ui: f_ui, Tahoma, Arial, sans-serif, -apple-system, BlinkMacSystemFont, Segoe UI, \
                Helvetica, Apple Color Emoji, Segoe UI Emoji

        // Generate v1 color variables.
        for _v in (bg fg ac) {
            for _i in 0..5 {
                key = s('--%s-%s', _v, _i)
                // Escape `\` for `*-0` vars.
                val = _v + \- + _i

                // Generate CSS variables.
                { key }: val
                // { key + '-rgb' }: to_rgb(val)
            }
        }

        // Generate v2 color variables.
        for tone in 0..9 {
            for number in 0..9 {
                key = '--v%s-%s' % ((tone) (number))
                val = '%s' % (v + tone + \- + number)
                { key }: val
            }
        }
    }
    if custom_font {
        * {
          font-family: var(--f-ui)
        }
        pre, textarea {
            font-family: var(--f-mono)
        }
    }

    // Syntax themes overrides
    if (cg_theme == 'solarized-light') {
        body {
            input {
                /.button {
                    &[name*="apply"] {
                        background: v44
                        border-color: v44
                        color: bg1
                        text-shadow: 0 1px 0 fg0 i
                        &:hover, &:focus-within {
                            background: v45 i
                            border-color: v45 i
                            color: bg0 i
                        }
                    }
                    &[name*="refresh"][style*="background"] {
                        c: 0 0 bg1
                        text-shadow: 0 1px 0 fg0
                    }
                    &[name*="reboot"] {
                        background: none repeat scroll 0 0 v16
                        border-color: v15
                        color: bg1
                        text-shadow: 0 1px 0 fg0
                        &:hover, &:focus-within {
                            color: bg0
                        }
                    }
                    &[name*="reset"] {
                        text-shadow: 0 1px 0 lighten(FG, 90%)
                        &:hover, &:focus-within {
                            text-shadow: 0 1px 0 lighten(FG, 90%)
                        }
                    }
                }
            }
        }
        .warning {
            background-color: v18 i
            border-color: v17 i
            color: bg4
        }
        // Images
        #header {
            // DDWRT logo
            #logo {
                filter: hue-rotate(180deg) contrast(1.2) i
                mix-blend-mode: exclusion i
            }
            .logo {
                filter: none
                mix-blend-mode: exclusion i
            }
        }
        div.note {
            background-color: v78 i
            color: bg5
        }
        // DD-WRT logo in about dialog
        [src*='dd-logo.png'] {
            filter: none
            mix-blend-mode: exclusion i
        }
    }

    // Try to hide default styles as not all are compatible and shouldnt be used together with style

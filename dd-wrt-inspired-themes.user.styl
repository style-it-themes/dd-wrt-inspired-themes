    'Dark blue (old)  ': 'old-colors',
    'Dark gray (new)  ': 'new-colors',
    'GitHub Dark      ': 'github-dark',
    'Breeze Dark      ': 'breeze-dark',
    'Twilight         ': 'twilight',
    'Ubuntu           ': 'ubuntu',
    'Solarized Dark   ': 'solarized-dark',
    'Solarized Light  ': 'solarized-light',
}
@var color    cg_bg        'Base background' #1f232a
@var color    cg_fg        'Base foreground' #eeeeee
@var color    cg_ac        'Base accent    ' #36c

@var range    cg_bg_a      'Adjust background tones' [0, -25, 25, 0.1, '%']
@var range    cg_fg_a      'Adjust foreground tones' [0, -25, 25, 0.1, '%']
@var range    cg_ac_a      'Adjust accent tones    ' [0,   0, 25,   1, '%']

@var text     f_ui         'UI font'                 ''font_name''
@var text     f_mono       'Monospace font'          ''font_name''
@var text     b_hgwd       'Checkbox width/height'   '1rem'

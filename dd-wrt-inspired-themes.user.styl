        dd {
            border-bottom: .123rem dotted bg5
        }

        // 401 Unauthorized page redesign
        if (new_401) {
            &[bgcolor="#cc9999"] {
                background-color: v11
                color: fg2

                font-size: 0
                font-family: Verdana, Helvetica
                text-align: center
                cursor: auto
                overflow: hidden i
                // Reposition & shorten text
                h4 {
                    &:hover {
                        animation: move-it 1.2s ease-in-out
                        cursor: not-allowed
                    }
                    margin-top: 3rem
                    font-size: 0
                    &::before {
                        content: '401\A'

                        font-size: 7rem
                        line-height: 10rem
                        text-shadow: 0 -1px 0 v99, 0 1px 0 v94,
                                     0 2px 0 v94,  0 3px 0 v94,
                                     0 4px 0 v94,  0 5px 0 v94,
                                     0 6px 0 v94,  0 7px 0 v94,
                                     0 8px 0 v94,  0 10px 5px rgba(BG, .4),
                                     0 20px 10px rgba(BG, .4)
                        white-space: pre-wrap
                    }
                    &::after {
                        content: 'UNAUTHORIZED'

                        animation: color_change 1s ease-in-out infinite alternate
                        font-variant: small-caps
                        font-size: 1.8rem
                        letter-spacing: 1px
                        line-height: 2rem
                    }
                }
                &::after {
                    r       = unquote
                    cbreak  = '\A '
                    isicon  = 'ðŸ›ˆ '
                    isnote  = 'Note that the default username is "root" in all newer releases'

                    content: '' + r(cbreak) + r(isicon) + r(isnote)
                    color: v99

                    font-size: 1rem
                    line-height: 1.3rem
                    text-shadow: 0 1px 0 v11
                    white-space: pre
                }
            }
        }
        else {
            &[bgcolor="#cc9999"] {
                background-color: v11
            }
        }
    }

    // Animations
    @keyframes color_change {
        0% {
            color: v10
            text-shadow: 0 1px 0 v12,
                         0 -1px 0 v12,
                         1px 0 0 v12,
                         -1px 0 0 v12,
                         0 -5px 5px v10,
                         0 5px 5px v10
        }
        50%, 100% {
            color: v12
            text-shadow: 0 1px 0 v16,
                         0 -1px 0 v16,
                         1px 0 0 v16,
                         -1px 0 0 v16,
                         0 -5px 5px v11,
                         0 5px 5px v11
        }
    }

    @keyframes move-it {
        0%   { transform: scale(1, 1)    translateY(0) }
        10%  { transform: scale(1.1, .9) translateY(0) }
        30%  { transform: scale(.9, 1.1) translateY(-10px) }
        50%  { transform: scale(1, 1)    translateY(0) }
        100% { transform: scale(1, 1)    translateY(0) }
    }


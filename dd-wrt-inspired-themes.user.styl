        #contents > form[name="setup"][autocomplete="new-password"] > br:nth-child(20) {
            display: none
        }
    }

    // WDS alignments
    form[name="wds"] {
        .label { margin-right: 20px }
        select { margin-right: 6px }
    }

    // Wol execution in progress box
    pre {
        background-color: bg0
    }

    // Body

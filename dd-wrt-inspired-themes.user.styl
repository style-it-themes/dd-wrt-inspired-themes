        text {
            /[fill="green"] { g: v44 }
            /[fill="red"] { g: v16 }
            /[fill="#435370"] { g: fg2 0 }
            /[fill="blue"] { g: v65 }
            /[fill="gray"] { g: fg5 }
        }
        path {
            /[stroke="red"] { g: 0 v16 }
            /[stroke="green"] { g: 0 v44}
            /[stroke='gray'] { g: 0 bg5; stroke-opacity: 1 }
        }
        rect {
            /#bg[fill="white"] { g: bg2; fill-opacity: 1 }
        }
    }

    // Traffic graph
    #t-graph {
        background: bg0
        border: 1px solid bg5
        color: fg3
        #ticks .tick { border-bottom: 1px solid bg5 }

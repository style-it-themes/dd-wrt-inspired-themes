        &[src*="hotspotsystem"] {
            r       = unquote
            prefix  = 'data:image/png;base64,'
            path    = ''
            ry      = 'no-repeat'

            background: s("url('%s%s') %s", r(prefix), r(path), r(ry))

            background-size: 170px
            padding-left: 170px
            padding-top: 25px
            margin-bottom: -10px
            margin-top: -5px

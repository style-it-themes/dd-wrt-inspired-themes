    .dialog {
        &_ {
            filter: invert(1)
        }
        &_content {
            color: fg1
            background-color: bg0
        }
    }

    // Progress dialog popup when applying settings
    .ddwrt {
        &_progress {
            filter: invert(1) grayscale(1)
        }

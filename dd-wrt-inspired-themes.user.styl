    }

    // Replace speedchecker terrible quality image
    img[src*="results_box"] {
        r       = unquote
        prefix  = 'data:image/png;base64,'
        path    = ''
        ry      = 'no-repeat'

        background: s("url('%s%s') %s", r(prefix), r(path), r(ry))

        background-size: 200px
        padding-left: 71%
        margin-top: 35px
        margin-bottom: -35px
        height: 0
        width: 0
    }
}

        }
        &_window {
            background: bg0
            border:1px solid v16
        }
        &_content {
            background: bg1
            color: fg1
        }
        &_title {
            color: v65
        }
        &_nw, &_n, &_e, &_sw, &_ne, &_w, &_s, &_se, &_sizer {
            background: bg1
        }
    }

    // Progress bar when e.g reboot
    .progressbarblock {
        background-color: ac0
    }

    .overlay_ddwrt {
        background-color: bg3
        opacity: .5
    }

    // Better checkbox/radio inputs.
    input {
        border: 1px solid bg4
        b: 0 1px 1px rgba(BG, .075)

        transition: transition
        padding: 7px

        &:hover, &:focus-within  {
            border-color: ac1
            background-color: bg1
        }

        r       = unquote
        prefix  = 'data:image/svg+xml;utf8,'
        prefix += '<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512">'

        /[type='radio'],
        /[type='checkbox'] {
            &, &:focus, &:hover:active {
                c: fg1 bg5 bg1

                appearance: none
                -moz-appearance: none
                -webkit-appearance: none
                background-size: cover
                bottom: .15rem
                cursor: pointer
                position: relative
                height: b_hgwd
                width: b_hgwd

                vertical-align: middle
            }
            &:disabled, &:hover:active:disabled, &:disabled:hover {
                c: fg1 bg5 bg4

                opacity: .7
                cursor: default
            }
        }

        /[type='radio'] {
            // fix border radius at https://github.com/settings/profile Emoji color
            border-radius: 50%
            &:checked {
                path = '<circle cx="256" cy="256" r="128" fill="' + to_svg(fg-0) + '"/>'

                background-image: s("url('%s%s</svg>') %s", r(prefix), r(path), i)
                c: 0 ac3 ac4

                padding: 7px
                &[disabled] {
                    c: fg1 bg5 bg4

                    opacity: .7
                }
            }
            &:focus { border-radius: 50% }
            &:hover { c: 0 ac1 bg2 }
        }

        /[type='checkbox'] {
            border-radius: 2px
            &:checked {
                path ='<path fill="' + to_svg(fg-0) + '" '
                path += 'd="M99 213l-1 89 104 102 205-201v-91L203 315 99 213z"/>'

                background-image: s("url('%s%s</svg>') %s", r(prefix), r(path), i)
                c: 0 ac3 ac4
                &[disabled] {
                    c: fg1 bg5 bg4

                    opacity: .7
                }
            }
            &:hover { c: 0 ac1 bg2 }
            &:indeterminate {
                path = '<rect x="128" y="128" width="256" height="256" fill="' + to_svg(fg-0) + '"/>'

                background-image: s("url('%s%s</svg>') %s", r(prefix), r(path), i)
                c: 0 v16 v14
            }
        }
        /[type="number"] { } // TODO: style spinbox
        // Style file browse button
        /[type='file'] {
            &::file-selector-button {
                background-color: bg5
                border: 2px solid bg5
                color: fg1

                border-radius: .32em
                padding: .2rem .4rem
                transition: transition
                cursor: pointer
                &:hover {
                    background-color: ac2
                    border: 2px solid ac2
                    box-shadow: 0 0 10px 0 ac4
                    color: fg1
                }
            }
            &::-webkit-file-upload-button {
                background-color: bg5
                border: 2px solid bg5
                color: fg1

                border-radius: .32em
                padding: .2rem .4rem
                transition: transition
                cursor: pointer
                &:hover {
                    background-color: ac2
                    border: 2px solid ac2
                    box-shadow: 0 0 10px 0 ac4
                    color: fg1
                }
            }
        }
        /[disabled] {
            opacity: .5
        }
    }

    .row[role="controls"] { border-color: bg4 }
    .progress {
        background-color: bg4
        box-shadow: inset 0 1px 2px rgba(BG, .1)
        -webkit-box-shadow: inset 0 1px 2px rgba(BG, .1)

        r: 4px
        &-bar {
            &.odd {
                background-color: ac1
            }
        }
    }

    // Better select element styles.
    select {
        // Custom select dropdown icon.
        r       = unquote
        prefix  = 'data:image/svg+xml;utf8,'
        prefix += '<svg width="16" height="16" fill="' + to_svg(fg-3) + '" '
        prefix += 'xmlns="http://www.w3.org/2000/svg">'
        path    = '<path d="M4.427 9.427l3.396 3.396a.251.251 0 00.354 '
        path   += '0l3.396-3.396A.25.25 0 0011.396 9H4.604a.25.25 0 '
        path   += '00-.177.427zM4.423 6.47L7.82 3.072a.25.25 0 01.354 0L11.57 '
        path   += '6.47a.25.25 0 01-.177.427H4.6a.25.25 0 01-.177-.427z"/>'
        xy      = 'no-repeat right/20px'

        background: s("url('%s%s</svg>') %s", r(prefix), r(path), r(xy) i)
        background-color: bg0 i
        border: 1px solid bg4
        b: 0px 1px 1px rgba(BG, .075)
        color: fg2

        appearance: none
        -moz-appearance: none
        -webkit-appearance: none
        background-position: right 3px center i
        background-size: 16px i
        height: 1.7rem
        max-width: 100%
        outline: 0
        padding-right: 1.2rem
        transition: transition
        option {
            background: bg0
            &:hover { background-color: ac3 }
        }
        &:focus-within  { c: 0 ac1 bg1 }
        &:hover {
            prefix  = 'data:image/svg+xml;utf8,'
            prefix += '<svg width="16" height="16" fill="' + to_svg(fg-1) + '" '
            prefix += 'xmlns="http://www.w3.org/2000/svg">'
            path    = '<path d="M4.427 9.427l3.396 3.396a.251.251 0 00.354 '
            path   += '0l3.396-3.396A.25.25 0 0011.396 9H4.604a.25.25 0 '
            path   += '00-.177.427zM4.423 6.47L7.82 3.072a.25.25 0 01.354 0L11.57 '
            path   += '6.47a.25.25 0 01-.177.427H4.6a.25.25 0 01-.177-.427z"/>'
            xy      = 'no-repeat right/20px'

            background: s("url('%s%s</svg>') %s", r(prefix), r(path), r(xy) i)
            c: 0 ac1 bg1

            background-position: right 3px center i
            background-size: 16px i
        }

        &[disabled] {
            r       = unquote
            prefix  = 'data:image/svg+xml;utf8,'
            prefix += '<svg width="16" height="16" fill="' + to_svg(bg-5) + '" '
            prefix += 'xmlns="http://www.w3.org/2000/svg">'
            path    = '<path d="M4.427 9.427l3.396 3.396a.251.251 0 00.354 '
            path   += '0l3.396-3.396A.25.25 0 0011.396 9H4.604a.25.25 0 '
            path   += '00-.177.427zM4.423 6.47L7.82 3.072a.25.25 0 01.354 0L11.57 '
            path   += '6.47a.25.25 0 01-.177.427H4.6a.25.25 0 01-.177-.427z"/>'
            xy      = 'no-repeat right/20px'

            background: s("url('%s%s</svg>') %s", r(prefix), r(path), r(xy) i)
            c: fg5 bg4 bg0

            background-position: right 3px center i
            background-size: 16px i
            opacity: .5
        }

        // QoS fix service name table width due to custom select menus.
        &[name="ServiceTable"] {
            background-color: bg0
            border: 1px solid bg4
            color: fg3

            width: inherit
            height: @width
        }
    }

    // fix bin
    .bin {
        r       = unquote
        prefix  = 'data:image/svg+xml;utf8,'
        prefix += '<svg viewBox="0 0 16 16" width="16" height="16" fill="' + to_svg(fg-5) + '" '
        prefix += 'xmlns="http://www.w3.org/2000/svg">'
        path    = '<path fill-rule="evenodd" d="M6.5 1.75a.25.25 0 01.25-.25h2.5a.25.25 0 01.25'
        path   += '.25V3h-3V1.75zm4.5 0V3h2.25a.75.75 0 010 1.5H2.75a.75.75 0 010-1.5H5V1.75C5 '
        path   += '.784 5.784 0 6.75 0h2.5C10.216 0 11 .784 11 1.75zM4.496 6.675a.75.75 0 10-1.'
        path   += '492.15l.66 6.6A1.75 1.75 0 005.405 15h5.19c.9 0 1.652-.681 1.741-1.576l.66-6'
        path   += '.6a.75.75 0 00-1.492-.149l-.66 6.6a.25.25 0 01-.249.225h-5.19a.25.25 0 01-.2'
        path   += '49-.225l-.66-6.6z"></path>'
        xy      = 'no-repeat center/15px'

        background: s("url('%s%s</svg>') %s", r(prefix), r(path), r(xy) i)
        &:hover {
           filter: brightness(.5) sepia(1) hue-rotate(300deg) saturate(40) brightness(1.1);
        }
    }

    // Increase speedchecker iframe height to fix a bug
    // where the city/town dropdown is cut off with max entries showing
    iframe[height="250"][src*="speedchecker"] {
        height: 293px
    }
}

                    opacity: .5
                }
            }
            &[name*="apply"] {
                background: none repeat scroll 0 0 v41
                border-color: v41
                color: fg1
                &:hover, &:focus-within {
                    background: none repeat scroll 0 0 v43 i
                    border-color: v43 i
                    box-shadow: 0 0 10px 0 v43
                    color: fg0 i
                }
            }
            &[name*="reset"] {
                background: none repeat scroll 0 0 bg3
                border-color: bg4
                color: fg1
                &:hover, &:focus-within {
                    background: none repeat scroll 0 0 bg5 i
                    border-color: bg5 i
                    box-shadow: 0 0 10px 0 bg5
                    color: fg0 i
                }
            }
            &[name*="refresh"][style*="background"] {
                c: 0 0 bg1
            }
            &[name*="reboot"] {
                background: none repeat scroll 0 0 v14
                border-color: v15
                color: fg1
                &:hover, &:focus-within {
                    background: none repeat scroll 0 0 v16 i
                    border-color: v17 i
                    box-shadow: 0 0 10px 0 v16
                }
            }
        }
        div {
            &.progressbar {
                background-color: bg4
                border: 1px solid bg5

                r: 4px
            }
        }
        dd {
            border-bottom: .123rem dotted bg5
        }

        // 401 Unauthorized page redesign make optional?
        @keyframes color_change {
            0% {
                color: fg2
                text-shadow: 0 0 0 v15, 1px -8px 3px rgba(BG, .4)
            }
            100% {
                color: v16
                text-shadow: 0 0 5px v16, 0 5px 3px rgba(BG, 2)
            }
        }
        @keyframes shake-it {
            from {
                transform: scale3d(1, 1, 1)
            }
            10%, 20% {
                transform: scale3d(.8, .8, .8) rotate3d(0, 0, 1, -2deg)
            }
            30%, 50% ,70% ,90% {
                transform: scale3d(1.1 ,1.1 ,1.1) rotate3d(0, 0, 1, 2deg)
            }
            40%, 60%, 80% {
                transform: scale3d(1, 1, 1) rotate3d(0, 0, 1, -2deg)
            }
            to {
                transform: scale3d(1.1, 1.1 ,1.1) rotate3d(0, 0, 1, 3deg)
            }
        }
